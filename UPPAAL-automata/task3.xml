<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>
const int N = 2;
typedef int[0,1] id_a;

chan advice[N], solution[N], detected[N],danger[N], manever[N];
urgent chan reception[N];
</declaration>
	<template>
		<name x="5" y="5">Sky</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="8" y="76">
			<name x="-25" y="42">Manoeuvre1</name>
		</location>
		<location id="id1" x="-289" y="-8">
			<name x="-306" y="-51">Safe</name>
		</location>
		<location id="id2" x="331" y="8">
			<name x="306" y="-34">Danger</name>
		</location>
		<location id="id3" x="0" y="-153">
			<name x="-17" y="-187">Danger1</name>
			<committed/>
		</location>
		<location id="id4" x="0" y="-68">
			<name x="-25" y="-110">Danger2</name>
			<committed/>
		</location>
		<location id="id5" x="8" y="153">
			<name x="-25" y="119">Manoeuvre2</name>
		</location>
		<init ref="id1"/>
		<transition>
			<source ref="id5"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="-271" y="55">manever[1]?</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="-178" y="8">manever[0]?</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="85" y="25">manever[1]?</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="178" y="76">manever[0]?</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="93" y="-34">danger[1]!</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="161" y="-102">danger[0]!</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id4"/>
			<label kind="synchronisation" x="-136" y="-42">danger[0]!</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-170" y="-119">danger[1]!</label>
		</transition>
	</template>
	<template>
		<name>Channel</name>
		<parameter>const id_a id</parameter>
		<location id="id6" x="-238" y="0">
			<name x="-248" y="-34">Wait</name>
		</location>
		<location id="id7" x="8" y="-8">
			<name x="-2" y="-42">On</name>
		</location>
		<init ref="id6"/>
		<transition>
			<source ref="id7"/>
			<target ref="id7"/>
			<label kind="synchronisation" x="21" y="-55">solution[1-id]?</label>
			<nail x="34" y="-68"/>
			<nail x="93" y="-8"/>
			<nail x="59" y="68"/>
		</transition>
		<transition>
			<source ref="id7"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="-161" y="42">reception[id]!</label>
			<nail x="-34" y="42"/>
			<nail x="-195" y="42"/>
		</transition>
		<transition>
			<source ref="id6"/>
			<target ref="id7"/>
			<label kind="synchronisation" x="-170" y="-102">solution[1-id]?</label>
			<nail x="-195" y="-76"/>
			<nail x="-51" y="-76"/>
		</transition>
	</template>
	<template>
		<name>Detector</name>
		<parameter>const id_a id</parameter>
		<declaration>clock t;</declaration>
		<location id="id8" x="0" y="0">
			<name x="-10" y="-34">Wait</name>
		</location>
		<location id="id9" x="-8" y="-144">
			<name x="-18" y="-178">Danger</name>
			<label kind="invariant" x="-25" y="-119">t &lt;= 60</label>
		</location>
		<init ref="id8"/>
		<transition>
			<source ref="id9"/>
			<target ref="id8"/>
			<label kind="synchronisation" x="-178" y="-85">detected[id]!</label>
			<nail x="-85" y="-76"/>
		</transition>
		<transition>
			<source ref="id8"/>
			<target ref="id9"/>
			<label kind="synchronisation" x="110" y="-93">danger[id]?</label>
			<label kind="assignment" x="110" y="-76">t := 0</label>
			<nail x="102" y="-68"/>
		</transition>
	</template>
	<template>
		<name>Resolution</name>
		<parameter>const id_a id</parameter>
		<declaration>clock c;
clock t;
int rand;</declaration>
		<location id="id10" x="-280" y="34">
			<name x="-331" y="25">Off</name>
		</location>
		<location id="id11" x="-25" y="34">
			<name x="-59" y="51">Compare</name>
			<urgent/>
		</location>
		<location id="id12" x="204" y="34">
			<name x="229" y="25">Wait</name>
			<label kind="invariant" x="238" y="51">t &lt;= 30</label>
		</location>
		<location id="id13" x="-51" y="-161">
			<name x="-85" y="-195">Calculation</name>
			<label kind="invariant" x="-76" y="-212">c &lt;= 5</label>
		</location>
		<init ref="id10"/>
		<transition>
			<source ref="id13"/>
			<target ref="id10"/>
			<label kind="guard" x="-441" y="-216">t &gt;= 30</label>
			<label kind="synchronisation" x="-441" y="-199">advice[id]!</label>
			<nail x="-459" y="-204"/>
			<nail x="-416" y="25"/>
		</transition>
		<transition>
			<source ref="id11"/>
			<target ref="id10"/>
			<label kind="guard" x="-195" y="8">rand &lt;= 7</label>
			<label kind="synchronisation" x="-195" y="42">advice[id]!</label>
		</transition>
		<transition>
			<source ref="id12"/>
			<target ref="id11"/>
			<label kind="select" x="85" y="34">x:int[0,10]</label>
			<label kind="synchronisation" x="25" y="17">reception[id]?</label>
			<label kind="assignment" x="17" y="34">rand := x</label>
		</transition>
		<transition>
			<source ref="id11"/>
			<target ref="id13"/>
			<label kind="guard" x="-38" y="-97">rand &gt; 7</label>
			<label kind="assignment" x="-25" y="-76">c := 0</label>
		</transition>
		<transition>
			<source ref="id12"/>
			<target ref="id10"/>
			<label kind="guard" x="-187" y="119">t &gt;= 30</label>
			<label kind="synchronisation" x="-187" y="136">advice[id]!</label>
			<nail x="177" y="153"/>
			<nail x="-205" y="153"/>
		</transition>
		<transition>
			<source ref="id13"/>
			<target ref="id12"/>
			<label kind="guard" x="76" y="-102">c&gt;1</label>
			<label kind="synchronisation" x="76" y="-85">solution[id]!</label>
		</transition>
		<transition>
			<source ref="id10"/>
			<target ref="id13"/>
			<label kind="synchronisation" x="-263" y="-76">detected[id]?</label>
			<label kind="assignment" x="-262" y="-63">t := 0, c:= 0</label>
		</transition>
	</template>
	<template>
		<name>Pilot</name>
		<parameter>const id_a id</parameter>
		<declaration>clock t;</declaration>
		<location id="id14" x="0" y="0">
			<name x="-34" y="25">Unbothered</name>
		</location>
		<location id="id15" x="-8" y="-144">
			<name x="-18" y="-178">Alerted</name>
			<label kind="invariant" x="-18" y="-127">t &lt;= 5</label>
		</location>
		<init ref="id14"/>
		<transition>
			<source ref="id15"/>
			<target ref="id14"/>
			<label kind="synchronisation" x="136" y="-85">manever[id]!</label>
			<nail x="127" y="-76"/>
		</transition>
		<transition>
			<source ref="id14"/>
			<target ref="id15"/>
			<label kind="synchronisation" x="-204" y="-102">advice[id]?</label>
			<label kind="assignment" x="-187" y="-68">t := 0</label>
			<nail x="-144" y="-59"/>
		</transition>
	</template>
	<system>system Sky, Detector, Pilot, Resolution, Channel;
    </system>
	<queries>
		<query>
			<formula>Sky.Danger1 --&gt; Sky.Manoeuvre1 or Sky.Manoeuvre2</formula>
			<comment></comment>
		</query>
		<query>
			<formula>
A[] not deadlock</formula>
			<comment></comment>
		</query>
		<query>
			<formula>Sky.Danger --&gt; Detector(0).Danger or Detector(1).Danger</formula>
			<comment></comment>
		</query>
	</queries>
</nta>
