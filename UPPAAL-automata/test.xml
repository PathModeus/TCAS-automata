<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.

const int N = 2;
typedef int[0,N-1] id_a;


chan manoeuvre[N], conseil[N], info[N], emission[N], danger[N], solution[N], infoVoisin[N]; 
</declaration>
	<template>
		<name>Pilot</name>
		<parameter>const id_a id</parameter>
		<declaration>clock t;</declaration>
		<location id="id0" x="-93" y="34">
			<name x="-127" y="51">Alerted</name>
			<label kind="invariant" x="-102" y="68">t &lt;= 5</label>
		</location>
		<location id="id1" x="-93" y="-93">
			<name x="-153" y="-127">Unbothered</name>
		</location>
		<init ref="id1"/>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-8" y="-51">conseil[id]?</label>
			<label kind="assignment" x="-17" y="-25">t := 0</label>
			<nail x="-8" y="-34"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="-272" y="-42">manoeuvre[id]!</label>
			<nail x="-170" y="-34"/>
		</transition>
	</template>
	<template>
		<name x="5" y="5">Aircraft</name>
		<parameter>const id_a id</parameter>
		<declaration>clock t;</declaration>
		<location id="id2" x="-51" y="59">
			<name x="-102" y="51">Safe</name>
		</location>
		<location id="id3" x="-93" y="-76">
		</location>
		<location id="id4" x="-93" y="-76">
		</location>
		<location id="id5" x="-93" y="-76">
			<name x="-153" y="-76">Danger</name>
		</location>
		<init ref="id5"/>
		<transition>
			<source ref="id5"/>
			<target ref="id3"/>
			<label kind="guard" x="-220" y="-178">t &gt; 1</label>
			<label kind="synchronisation" x="-220" y="-161">info[id]!</label>
			<label kind="assignment" x="-220" y="-144">t := 0</label>
			<nail x="-85" y="-144"/>
			<nail x="-238" y="-144"/>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id5"/>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id3"/>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id5"/>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-17" y="-25">manoeuvre[id]?</label>
			<nail x="-25" y="-17"/>
			<nail x="-25" y="-17"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-127" y="-8">info[id]!</label>
			<label kind="assignment" x="-127" y="8">t := 0</label>
		</transition>
	</template>
	<template>
		<name>Sensor</name>
		<parameter>const id_a id</parameter>
		<location id="id6" x="-153" y="-17">
			<name x="-127" y="-8">Retransmission</name>
			<urgent/>
		</location>
		<location id="id7" x="-348" y="-76">
			<name x="-408" y="-102">Wait</name>
		</location>
		<init ref="id7"/>
		<transition>
			<source ref="id6"/>
			<target ref="id7"/>
			<label kind="synchronisation" x="-238" y="-178">emission[id]!</label>
			<nail x="-119" y="-127"/>
			<nail x="-331" y="-178"/>
		</transition>
		<transition>
			<source ref="id7"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="-314" y="-8">info[1-id]?</label>
			<nail x="-306" y="-25"/>
			<nail x="-204" y="0"/>
		</transition>
		<transition>
			<source ref="id7"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="-246" y="-93">info[id]?</label>
			<nail x="-297" y="-85"/>
			<nail x="-178" y="-59"/>
		</transition>
	</template>
	<template>
		<name>ConflictDetection</name>
		<parameter>const id_a id</parameter>
		<location id="id8" x="-161" y="-144">
			<name x="-170" y="-127">Off</name>
		</location>
		<location id="id9" x="-161" y="-263">
			<name x="-171" y="-297">On</name>
			<urgent/>
		</location>
		<init ref="id8"/>
		<transition>
			<source ref="id9"/>
			<target ref="id8"/>
			<label kind="synchronisation" x="-68" y="-212">danger[id]!</label>
			<nail x="-76" y="-195"/>
		</transition>
		<transition>
			<source ref="id8"/>
			<target ref="id9"/>
			<label kind="synchronisation" x="-340" y="-212">emission[id]?</label>
			<nail x="-255" y="-195"/>
		</transition>
	</template>
	<template>
		<name>Channel</name>
		<parameter>const id_a id</parameter>
		<location id="id10" x="-297" y="-34">
			<name x="-340" y="-42">Off</name>
		</location>
		<location id="id11" x="-51" y="-102">
			<name x="-61" y="-136">Communique</name>
			<urgent/>
		</location>
		<init ref="id10"/>
		<transition>
			<source ref="id11"/>
			<target ref="id10"/>
			<label kind="synchronisation" x="-178" y="0">infoVoisin[1-id]!</label>
			<nail x="-136" y="0"/>
		</transition>
		<transition>
			<source ref="id10"/>
			<target ref="id11"/>
			<label kind="synchronisation" x="-280" y="-178">solution[id]?</label>
			<nail x="-229" y="-153"/>
		</transition>
	</template>
	<template>
		<name>ConflictResolution</name>
		<parameter>const id_a id</parameter>
		<declaration>clock c;
clock t;


</declaration>
		<location id="id12" x="25" y="-221">
			<name x="-25" y="-255">Calcul</name>
			<label kind="invariant" x="25" y="-255">c &lt;=5 and t &lt;= 30</label>
		</location>
		<location id="id13" x="-289" y="25">
			<name x="-297" y="42">Off</name>
		</location>
		<location id="id14" x="34" y="-68">
			<name x="0" y="-42">Comparaison</name>
			<label kind="invariant" x="24" y="-51">t &lt;= 30</label>
		</location>
		<location id="id15" x="263" y="0">
			<name x="253" y="-34">Wait</name>
			<label kind="invariant" x="253" y="17">t &lt;= 30</label>
		</location>
		<init ref="id13"/>
		<transition>
			<source ref="id14"/>
			<target ref="id13"/>
			<label kind="select" x="-144" y="-17">x:int[0,10]</label>
			<label kind="guard" x="-170" y="-34">x &gt; 7 or t &gt;= 30</label>
			<label kind="synchronisation" x="-127" y="-51">conseil[id]!</label>
		</transition>
		<transition>
			<source ref="id12"/>
			<target ref="id15"/>
			<label kind="synchronisation" x="161" y="-127">solution[id]!</label>
		</transition>
		<transition>
			<source ref="id15"/>
			<target ref="id14"/>
			<label kind="synchronisation" x="85" y="-68">infoVoisin[id]?</label>
		</transition>
		<transition>
			<source ref="id15"/>
			<target ref="id13"/>
			<label kind="guard" x="17" y="34">t&gt;=30</label>
			<label kind="synchronisation" x="8" y="17">conseil[id]!</label>
		</transition>
		<transition>
			<source ref="id14"/>
			<target ref="id12"/>
			<label kind="guard" x="-17" y="-144">t &lt; 25</label>
			<label kind="assignment" x="-8" y="-119">c := 0</label>
			<nail x="34" y="-119"/>
			<nail x="25" y="-170"/>
		</transition>
		<transition>
			<source ref="id13"/>
			<target ref="id12"/>
			<label kind="synchronisation" x="-178" y="-127">danger[id]?</label>
			<label kind="assignment" x="-229" y="-110">t := 0, c := 0</label>
			<nail x="-110" y="-110"/>
		</transition>
	</template>
	<system>system Pilot, Aircraft, Sensor, ConflictDetection, Channel, ConflictResolution;
    </system>
	<queries>
		<query>
			<formula>E&lt;&gt; Aircraft[1].Safe</formula>
			<comment></comment>
		</query>
	</queries>
</nta>
