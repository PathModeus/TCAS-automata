		<location id="id10" x="-263" y="25">
			<name x="-273" y="-9">Safe01</name>
			<committed/>
		</location>
		<location id="id11" x="-263" y="102">
			<name x="-273" y="68">Safe10</name>
			<committed/>
		</location>
		<location id="id12" x="93" y="-229">
			<name x="59" y="-263">Danger10</name>
